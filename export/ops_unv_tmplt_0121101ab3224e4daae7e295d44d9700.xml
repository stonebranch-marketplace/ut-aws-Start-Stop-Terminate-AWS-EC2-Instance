#/opt/universal/python3.6/bin/python3
###########################################################
#    Name: EC2_Start_Stop_Instance.py
#
#    Purpose: 
#           1. Start EC2 Instances 
#           2. Stop EC2 Instances
#           3. Terminate EC2 Instances
#
#    Prerequisites:
#     - Universal Agent for Windows/Linux
#     - Python 2.7.x
#     - Requires the Python modules
#     - datetime, boto3 , botocore, argparse, sys
#     - Note: module can be added using pip
#     - e.g. use: pip install boto3
#
#     Version History:
#     0.1  Ravi Murugesan 2020-04-19 Initial Version
#     0.2  Ravi Murugesan 2020-05-28 Info detail update
####################################################
version = "0.2"

# Used modules

import re
import glob
import sys, os
import datetime, time
import logging
import boto3, botocore
import argparse

# -- loglevel settings INFO,DEBUG, WARNING, ERROR, CRITICAL

logging.basicConfig(level=logging.INFO, format=' %(asctime)s - %(levelname)s - %(message)s')

def main():
    Set_Variables()  # -- Set Variables from Universal Task Form or Command Line Input
    if args.ec2_instance_option == "start_ec2":
        start_ec2_instance()
    elif args.ec2_instance_option == "stop_ec2":
        stop_ec2_instance()
    elif args.ec2_instance_option == "terminate_ec2":
        terminate_ec2_instance()
    elif args.ec2_instance_option == "ec2_status":
        ec2_instance_status()
# --
def Set_Variables():
    parser = argparse.ArgumentParser(description='Purpose : Create EC2 Instance')
    # ## --&gt; AWS Connection Settings:
    parser.add_argument("--AWS_ACCESS_KEY_ID", default="${_credentialPwd('${ops_ec2_aws_access_key_id}')}")
    parser.add_argument("--AWS_SECRET_ACCESS_KEY", default="${_credentialPwd('${ops_ec2_aws_secret_access_key}')}")
    parser.add_argument("--AWS_DEFAULT_REGION", default="${_credentialPwd('${ops_ec2_aws_default_region}')}")

    # ## --&gt;EC2 API specific Settings
    parser.add_argument("--ec2_instance_option", default="${ops_ec2_ec2_instance_option}")
    parser.add_argument("--instance_ids", default="${ops_ec2_instance_ids}")

    # ## --
    global args
    args = parser.parse_args()
    # -- Setup Logging
    logging.info("Executing version " + version + " with the following paramaters")
    logging.info(args)
# --
# --
### Make Resource connection 
def make_resource_connection():
    from botocore.config import Config
    logging.info('EC2 Resource connection')
    ec2resource = boto3.client(
            'ec2',
            aws_access_key_id=args.AWS_ACCESS_KEY_ID,
            aws_secret_access_key=args.AWS_SECRET_ACCESS_KEY,
            region_name=args.AWS_DEFAULT_REGION
    )
    return ec2resource
##################################################################################################
# Use Boto3 Function to start ec2 instance(s) 
#
##################################################################################################
def start_ec2_instance():
    # Establish connection to AWS
    ########################################################
    ec2resource = make_resource_connection()
    #Format the instanceid input for the boto3 function
    instances=args.instance_ids.split(',')
    try:
        logging.info("------------------------------")
        logging.info("Initiating EC2 Instance start ")
        logging.info("------------------------------")
        waiter = ec2resource.get_waiter('instance_running')
        response=ec2resource.start_instances(
        InstanceIds=instances)
        logging.info(response)
        logging.info("Waiting until instance is running")
        waiter.wait(InstanceIds=instances)
        logging.info("Instances: "+args.instance_ids+" is in running status")
        logging.info(" Check Instance status")
        waiter = ec2resource.get_waiter('instance_status_ok')
        waiter.wait(InstanceIds=instances)
        logging.info("Instance Status Check Completed")
    except Exception as e:
        logging.error("Error in starting ec2 instance")
        logging.error(e)
        sys.exit(1)
#################################################################
# Function to stop EC2 instance(s)
#################################################################
def stop_ec2_instance():
    # Establish connection to AWS
    ########################################################
    ec2resource = make_resource_connection()
    #Format the instanceid input for the boto3 function
    instances=args.instance_ids.split(',')
    try:
        logging.info("----------------------------")
        logging.info("Initiating EC2 Instance Stop")
        logging.info("----------------------------")
        waiter = ec2resource.get_waiter('instance_stopped')
        response=ec2resource.stop_instances(
        InstanceIds=instances)
        logging.info(response)
        waiter.wait(InstanceIds=instances)
        logging.info("Instances: "+args.instance_ids+" is Stopped")
    except Exception as e:
        logging.error("Error in stopping ec2 instance")
        logging.error(e)
        sys.exit(1)
#################################################################
# Function to Terminate EC2 instance(s)
#################################################################
def terminate_ec2_instance():
    # Establish connection to AWS
    ########################################################
    ec2resource = make_resource_connection()
    #Format the instanceid input for the boto3 function
    instances=args.instance_ids.split(',')
    try:
        logging.info("-----------------------------------")
        logging.info("Initiating EC2 Instance Termination")
        logging.info("-----------------------------------")
        waiter = ec2resource.get_waiter('instance_terminated')
        response=ec2resource.terminate_instances(
        InstanceIds=instances)
        logging.info(response)
        waiter.wait(InstanceIds=instances)
        logging.info("Instances: "+args.instance_ids+" is terminated")
    except Exception as e:
        logging.error("Error in Terminating ec2 instance")
        logging.error(e)
        sys.exit(1)
#################################################################
# Function to Check EC2 instance(s) status
#################################################################
def ec2_instance_status():
    # Establish connection to AWS
    ########################################################
    ec2resource = make_resource_connection()
    #Format the instanceid input for the boto3 function
    instances=args.instance_ids.split(',')
    try:
        logging.info("-----------------------------------")
        logging.info("Initiating EC2 Instance Status")
        logging.info("-----------------------------------")
        response=ec2resource.describe_instance_status(
        InstanceIds=instances)
        logging.info(response)
        for instance in response['InstanceStatuses']:
            logging.info("Instance ID :"+instance['InstanceId']+" Instance State:"+instance['InstanceState']['Name']+" Instance Status:"+instance['InstanceStatus']['Status']+" System Status:"+instance['SystemStatus']['Status'])
    except Exception as e:
        logging.error("Error in checking ec2 instance status")
        logging.error(e)
        sys.exit(1)
# -- Execute
